{
	"name": "betterbox",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "dotenv -e .env -- next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:seed": "dotenv -- tsx src/db/seed.ts",
		"db:studio": "drizzle-kit studio",
		"dev": "dotenv -e .env -- next dev --turbo",
		"dev:mock": "cross-env USE_MOCK_UNIPILE=1 NEXT_PUBLIC_USE_MOCK_UNIPILE=1 dotenv -e .env -- next dev --turbo",
		"msw:build": "msw init public --save",
		"postinstall": "drizzle-kit generate",
		"preview": "dotenv -e .env -- next build && dotenv -e .env -- next start",
		"start": "dotenv -e .env -- next start",
		"typecheck": "tsc --noEmit",
		"inngest:dev": "pnpm dlx inngest-cli@latest dev",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"test:mocks": "vitest run src/mocks/__tests__",
		"test:mocks:watch": "vitest src/mocks/__tests__",
		"prepare": "husky",
		"inngest": "npx inngest-cli@latest dev"
	},
